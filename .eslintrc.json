{
    "plugins": ["import"],
    "extends": ["next/core-web-vitals", "plugin:import/typescript"],
    "rules": {
        "import/order": [
            1,
            {
                "groups": [
                    "builtin",
                    "type",
                    "external",
                    "parent",
                    "sibling",
                    "index"
                ],
                "newlines-between": "always"
            }
        ]
    }
}
